@model EducationalPortal.Application.DTO.CourseDTO

@{
    ViewData["Title"] = "Create Course";
}

<h1>Create Course</h1>

<div class="row">
    <div class="col-md-4">
        <div id="createCourse">
            @await Html.PartialAsync("_CreateCourse", Model)
        </div>
        <input type="button" value="Create" class="btn btn-success my-2" onclick="CreateCourse()"/>
    </div>
    <div class="col-md-8" id="addPanel"></div>
</div>